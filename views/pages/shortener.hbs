<div class="container-fluid">
  <div class="row" style="background-color: white; box-shadow: 0 1px 5px #ccc">
    <div class="col py-4">
      <p class="display-5 text-primary text-center" style="font-weight: 800">
        Short URL
      </p>
    </div>
  </div>
  {{#if invalid}}
  {{> invalidURL}}
  {{else if valid}}
  {{> validURL}}
  {{else if totalClicks}}
  {{> totalClicks}}
  {{else}}
  {{> totalClicksCount}}
  {{/if}}
</div>
